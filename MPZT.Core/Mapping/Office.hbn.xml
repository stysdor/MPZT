<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="MPZT.Core.Domain" assembly="MPZT.Core" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Office" table="Offices">
    <id name="Id" column="Id" type="Int32">
      <generator class="native" />
    </id>
    <property name="OfficeName" type="String" column="OfficeName" length="255" />
    <property name="OfficeAddress" type="String" column="OfficeAddress" length="255" />
    <property name="OfficePhone" type="String" column="OfficePhone" length="255" />
    <property name="OfficeEmail" type="String" column="OfficeEmail" length="255" />
  </class>
</hibernate-mapping>
